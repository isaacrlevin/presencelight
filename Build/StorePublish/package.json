    "streamifier": "^0.1.1",
    "url": "^0.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^13.7.1",
    "@types/uuid": "^8.3.0",
    "del": "^2.2.2",
    "shelljs": "^0.7.3",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
